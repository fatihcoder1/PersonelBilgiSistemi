@using PersonelBilgiSistemi.Models
@model List<Personel>
@{
    ViewData["Title"] = "BirimDetay";
    Layout = "~/Views/Shared/_GenelLayout.cshtml";
}
<br />
<h3>@ViewBag.brm Birim Detay Sayfası </h3>
<br />


<br />
<table class="table table-bordered">
    <tr>
        <th>Personel Id</th>
        <th>Adı</th>
        <th>Soyadı</th>
        <th>Sehir</th>
        
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PersonelId</td>
            <td>@item.Adi</td>
            <td>@item.Soyadi</td>
            <td>@item.Sehir</td>
            
        </tr>
    }
</table>

